<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:scrollbarAlwaysDrawHorizontalTrack="true"
    tools:context=".ViewTransactionFragment"
    tools:layout_editor_absoluteX="4dp"
    tools:layout_editor_absoluteY="4dp">

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Education fund"
        android:textSize="20sp"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="202dp" />

    <TextView
        android:id="@+id/editTextNumber"
        android:layout_width="160dp"
        android:layout_height="42dp"
        android:ems="10"
        android:inputType="number"
        android:text="50000"
        android:textSize="20sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/textView2"
        tools:ignore="MissingConstraints,TouchTargetSizeCheck"
        tools:layout_editor_absoluteY="202dp" />

    <TextView
        android:id="@+id/editTextTextPersonName10"
        android:layout_width="155dp"
        android:layout_height="48dp"
        android:textSize="20sp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="24dp"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="Ram"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView2" />

    <TextView
        android:id="@+id/editTextTextPersonName11"
        android:layout_width="149dp"
        android:textSize="20sp"
        android:layout_height="48dp"
        android:layout_marginStart="4dp"
        android:layout_marginTop="20dp"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="Devid"
        app:layout_constraintStart_toStartOf="@+id/editTextTextPersonName10"
        app:layout_constraintTop_toBottomOf="@+id/editTextTextPersonName10" />

    <TextView
        android:id="@+id/editTextTextPersonName12"
        android:layout_width="162dp"
        android:layout_height="48dp"
        android:textSize="20sp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="12dp"
        android:ems="10"

        android:inputType="textPersonName"
        android:text="Dip"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/editTextTextPersonName11"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/editTextTextPersonName13"
        android:layout_width="170dp"
        android:textSize="20sp"
        android:layout_height="48dp"
        android:layout_marginStart="20dp"
        android:layout_marginTop="24dp"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="Anjan"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/editTextTextPersonName12"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/editTextTextPersonName14"
        android:layout_width="162dp"
        android:layout_height="48dp"
        android:textSize="20sp"
        android:layout_marginStart="20dp"
        android:layout_marginTop="28dp"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="Anupam"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/editTextTextPersonName13"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/editTextNumberDecimal"
        android:layout_width="163dp"
        android:layout_height="48dp"
        android:textSize="20sp"
        android:layout_marginTop="9dp"
        android:ems="10"
        android:inputType="numberDecimal"
        android:text="7000"
        app:layout_constraintEnd_toEndOf="@+id/editTextNumber"
        app:layout_constraintStart_toStartOf="@+id/editTextNumber"
        app:layout_constraintTop_toBottomOf="@+id/editTextNumber"
        tools:ignore="SpeakableTextPresentCheck" />

    <TextView
        android:id="@+id/editTextNumberDecimal2"
        android:layout_width="166dp"
        android:textSize="20sp"
        android:layout_height="48dp"
        android:layout_marginStart="72dp"
        android:layout_marginTop="15dp"
        android:layout_marginEnd="80dp"
        android:layout_marginBottom="5dp"
        android:ems="10"
        android:inputType="numberDecimal"
        android:text="90000"
        app:layout_constraintBottom_toBottomOf="@+id/editTextTextPersonName11"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/editTextTextPersonName11"
        app:layout_constraintTop_toBottomOf="@+id/editTextNumberDecimal"
        app:layout_constraintVertical_bias="0.375"
        tools:ignore="SpeakableTextPresentCheck" />

    <TextView
        android:id="@+id/editTextNumberDecimal3"
        android:layout_width="160dp"
        android:layout_height="48dp"
        android:layout_marginTop="145dp"
        android:ems="10"
        android:inputType="numberDecimal"
        android:text="70800"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="@+id/editTextTextPersonName12"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.402"
        app:layout_constraintStart_toEndOf="@+id/editTextTextPersonName12"
        app:layout_constraintTop_toBottomOf="@+id/editTextNumber"
        tools:ignore="SpeakableTextPresentCheck" />

    <TextView
        android:id="@+id/editTextNumberDecimal4"
        android:layout_width="164dp"
        android:layout_height="48dp"
        android:textSize="20sp"
        android:layout_marginStart="13dp"
        android:layout_marginTop="20dp"
        android:ems="10"
        android:inputType="numberDecimal"
        android:text="58000"
        app:layout_constraintEnd_toEndOf="@+id/editTextNumberDecimal3"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toEndOf="@+id/editTextTextPersonName13"
        app:layout_constraintTop_toBottomOf="@+id/editTextNumberDecimal3"
        tools:ignore="SpeakableTextPresentCheck" />

    <TextView
        android:id="@+id/editTextNumberDecimal5"
        android:layout_width="189dp"
        android:layout_height="48dp"
        android:layout_marginTop="28dp"
        android:ems="10"
        android:inputType="numberDecimal"
        android:text="8900"
        android:textSize="20sp"
        app:layout_constraintEnd_toEndOf="@+id/editTextNumberDecimal4"
        app:layout_constraintStart_toStartOf="@+id/editTextNumberDecimal4"
        app:layout_constraintTop_toBottomOf="@+id/editTextNumberDecimal4"
        tools:ignore="SpeakableTextPresentCheck" />


</androidx.constraintlayout.widget.ConstraintLayout>